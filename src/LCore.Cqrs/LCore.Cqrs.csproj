<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Lumini CQRS. Commands and Queries based on Requests</Description>
    <TargetFramework>net10.0</TargetFramework>
    <AssemblyName>Lumini.Core.Cqrs</AssemblyName>
    <RootNamespace>Lumini.Core.Cqrs</RootNamespace>
    <PackageTags>$(PackageTags);Lumini CQRS</PackageTags>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="10.0.3" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.3" />
  </ItemGroup>

</Project>
